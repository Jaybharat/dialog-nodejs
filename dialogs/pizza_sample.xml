<?xml version="1.0" encoding="UTF-8"?>
<dialog xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<flow>
		<!-- 1st step -->
		<folder label="Main">
			<output>
				<prompt selectionType="RANDOM">
					<item>Hi, I'm B2B-Watson! I can help in doing Pre-assessment on your boarding request. Would you like me to process ? 
					</item>
				</prompt>
				<goto ref="getUserInput_2449614" />
			</output>
			<output>
				<prompt selectionType="SEQUENTIAL">
					<item>Is there anything I can help you with?</item>
				</prompt>
				<getUserInput id="getUserInput_2449614">
					<search ref="folder_2449611" />
					<default>
						<output>
							<prompt selectionType="RANDOM">
								<item>I'm sorry I did not understand your question.</item>
								<item>I did not quite get that.</item>
								<item>I didn't grasp what you said.</item>
								<item>I'm not following you at the moment.</item>
							</prompt>
						</output>
					</default>
				</getUserInput>
			</output>
		</folder>

		<!-- 2nd step -->
		<folder label="Library">
			<folder label="Live Content" id="folder_2449611">
				<input>
					<grammar>
						<item>Yes, of course I would like you to do TPA for me.</item>
						<item>$ Yes</item>
						<item>$ Of course</item>
						<item>$ Sure</item>
					</grammar>
					<output>
						<prompt selectionType="RANDOM">
							<item>I will ask you a list of questions, please answer them with acurate information, for choices use Yes or No or NA where applicable, So can we start the assessment ?</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>Yes</item>
									<item>$ Sure</item>
									<item>$ OK</item>
									<item>$ Of course</item>
									<item>$ Yes</item>
								</grammar>
								<goto ref="output_2449612" />
							</input>
						</getUserInput>
					</output>

					<!-- For Client Name -->
					<output id="output_2449612">
						<prompt selectionType="RANDOM">
							<item>What is the name of the Client you are requesting to board?</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>Client Name is </item>
									<item>$ Client Name is (Entity_ClientName)={ClientName}</item>
									<item>$ (Entity_ClientName)={ClientName}</item>
								</grammar>
								<action varName="ClientName" operator="SET_TO">{ClientName.source}
								</action>
								<goto ref="output_2443126" />
							</input>
						</getUserInput>
					</output>

					<!-- For Country Scope -->
					<output id="output_2443126">
						<prompt>
							<item>Please provide the Country in scope from the following list.
							</item>
						</prompt>
						<goto ref="output_2443128" />
					</output>
					<output id="output_2443128">
						<prompt selectionType="RANDOM">
							<item>USA, France, Germany, Netherlands,Belgium, UK </item>
						</prompt>
						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_CountryScope)={CountryScope}</item>
								</grammar>
								<action varName="CountryScope" operator="SET_TO">{CountryScope.value:main}
								</action>
								<goto ref="output_2443129" />
							</input>
						</getUserInput>
					</output>

					<!-- For Contact -->
					<output id="output_2443129">
						<prompt>
							<item>Thanks, Please provide Customer IT  Contact email id or
								Phone No. (mandatory)
							</item>
						</prompt>
						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_Contact)={Contact}</item>
								</grammar>
								<action varName="Contact" operator="SET_TO">{Contact.source}
								</action>
								<goto ref="output_2443130" />
							</input>

						</getUserInput>
					</output>


					<!-- For Brand -->
					<output id="output_2443130">
						<prompt>
							<item>Thanks, Please provide the applicable IBM brands in scope .
							</item>
						</prompt>
						<goto ref="output_2443131" />
					</output>
					<output id="output_2443131">
						<prompt selectionType="RANDOM">
							<item>Configured Hardware ,System x, 
								Hardware, Distributed Software, Entitled Software,
								Services, Maintenance or Warranties, Leasing, Education Services, Other,
								International Global Financing (IGF)
							</item>
						</prompt>
						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_Brand)={Brand}</item>
								</grammar>
								<action varName="Brand" operator="SET_TO">{Brand.value:main}
								</action>
								<goto ref="output_2443132" />
							</input>
						</getUserInput>
					</output>
					<!-- <output id="output_2443156"> <prompt> <item>Please provide the 
						applicable IBM brands to enable for purchasing / billing electronically from 
						the following list. </item> </prompt> <goto ref="output_2443131" /> </output> -->
					<!-- For Message -->
					<output id="output_2443132">
						<prompt>
							<item>Thanks, Please provide the requested message for Boarding from the
								following list.
							</item>
						</prompt>
						<goto ref="output_2443133" />
					</output>
					<output id="output_2443133">
						<prompt selectionType="RANDOM">
							<item>Order Confirmation, Order Response, Advanced Shipment
								Notice, Orders and Change Orders,
								Invoice and Credit
							</item>
						</prompt>
						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_Message)={Message}</item>
								</grammar>
								<action varName="Message" operator="SET_TO">{Message.value:main}
								</action>
								<goto ref="output_2443134" />
							</input>
						</getUserInput>
					</output>

					<!-- For PaperSample_Available -->
					<output id="output_2443134">
						<prompt>
							<item>Thanks, You need to share atleast 1 test Paper Sample per LOB, Please confirm if you can share it ?
							</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsPaperSampleAvailable)={IsPaperSampleAvailable}
									</item>
								</grammar>
								<action varName="IsPaperSampleAvailable" operator="SET_TO">{IsPaperSampleAvailable.value:main}
								</action>
								<goto ref="output_2443135" />
							</input>
						</getUserInput>
					</output>

					<!-- For CustomerNum -->
					<output id="output_2443135">
						<prompt>
							<item>Thanks, You need to Provide Confirmation on customer numbers in scope, Do you have all the customer # in Scope ?
							</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_CustomerNum)={CustomerNum}</item>
								</grammar>
								<action varName="CustomerNum" operator="SET_TO">{CustomerNum.source}
								</action>
								<goto ref="output_2443136" />
							</input>
						</getUserInput>
					</output>

					<!-- For HVECScope -->
					<output id="output_2443136">
						<prompt>
							<item>Thanks, Please Provide Confirm if Hardware (HVEC) is in Scope ? </item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_HVECScope)={HVECScope}</item>
								</grammar>
								<action varName="HVECScope" operator="SET_TO">{HVECScope.value:main}
								</action>
								<goto ref="output_2443137" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsRevenueReportThere -->
					<output id="output_2443137">
						<prompt>
							<item>Thanks, You need to have latest Revenue report of last 12 months, Do you have the latest one ready?</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsRevenueReportThere)={IsRevenueReportThere}
									</item>
								</grammar>
								<action varName="IsRevenueReportThere" operator="SET_TO">{IsRevenueReportThere.value:main}
								</action>
								<goto ref="output_2443138" />
							</input>
						</getUserInput>
					</output>

					<!-- For SiteID -->
					<output id="output_2443138">
						<prompt>
							<item>Thanks, Please provide Site ID in scope.if any </item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_SiteID)={SiteID}</item>
								</grammar>
								<action varName="SiteID" operator="SET_TO">{SiteID.source}
								</action>
								<goto ref="output_2443139" />
							</input>
						</getUserInput>
					</output>

					<!-- For IBMEntity -->
					<output id="output_2443139">
						<prompt>
							<item>Thanks, Please provide IBM Entity in Scope , check with the CRR/OLC for the information</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IBMEntity)={IBMEntity}</item>
								</grammar>
								<action varName="IBMEntity" operator="SET_TO">{IBMEntity.source}
								</action>
								<goto ref="output_2443140" />
							</input>
						</getUserInput>
					</output>

					<!-- For EPV -->
					<output id="output_2443140">
						<prompt>
							<item>Thanks, Now lets understand, How would client like to connect with IBM Gateway, Are you requesting to connect with IBM directly or want to go via an ePV. Note: ePV are the service provider works between Buyer and Supplier and handle Ordering and Invoicing on behalf of Buyers.
							</item>
						</prompt>
						<goto ref="output_2443141" />
					</output>
					<output id="output_2443141">
						<prompt selectionType="RANDOM">
							<item>Direct connection, ePV- Ariba, ePV - Tungsten, ePV- Bprocess, ePV- Basware</item>
						</prompt>
						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_EPV)={EPV}</item>
								</grammar>
								<action varName="EPV" operator="SET_TO">{EPV.value:main}
								</action>
								<goto ref="output_2443142" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsMsgImplGuideMandatory -->
					<output id="output_2443142">
						<prompt>
							<item>Thanks, Do the client /ePV  have a Message Implementation guide (mandatory for Customer
								requesting a Direct connectivity)?
							</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsMsgImplGuideMandatory)={IsMsgImplGuideMandatory}
									</item>
								</grammar>
								<action varName="IsMsgImplGuideMandatory" operator="SET_TO">{IsMsgImplGuideMandatory.value:main}
								</action>
								<goto ref="output_2443143" />
							</input>
						</getUserInput>
					</output>

					<!-- For EnvoicingSolution -->
					<output id="output_2443143">
						<prompt>
							<item>Thanks, what preferable electronic solution you would like to Opt ?
							</item>
						</prompt>
						<goto ref="output_2443144" />
					</output>
					<output id="output_2443144">
						<prompt selectionType="RANDOM">
							<item>ESI eIDM Integrated, Create From Blank, Sterling Webform,
								PO Flip, CIS MIV Integrated, LIW, SAP/BH
							</item>
						</prompt>
						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_EnvoicingSolution)={EnvoicingSolution}</item>
								</grammar>
								<action varName="EnvoicingSolution" operator="SET_TO">{EnvoicingSolution.value:main}
								</action>
								<goto ref="output_2443145" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsTechSampleMandatory -->
					<output id="output_2443145">
						<prompt>
							<item>Do you have a technical sample (CXML or EDI) that you can share ?(mandatory for a PO Flip Webform solution)
							</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsTechSampleMandatory)={IsTechSampleMandatory}
									</item>
								</grammar>
								<action varName="IsTechSampleMandatory" operator="SET_TO">{IsTechSampleMandatory.value:main}
								</action>
								<goto ref="output_2443146" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsInvoiceReqd -->
					<output id="output_2443146">
						<prompt>
							<item>Thanks, For invoicing, Do you require invoice with attachments?</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsInvoiceReqd)={IsInvoiceReqd}</item>
								</grammar>
								<action varName="IsInvoiceReqd" operator="SET_TO">{IsInvoiceReqd.value:main}
								</action>
								<goto ref="output_2443147" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsInvoiceReqdinMultplCurrency -->
					<output id="output_2443147">
						<prompt>
							<item>Thanks, For invoicing, Do you require invoices in multiple currencies?</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsInvoiceReqdinMultplCurrency)={IsInvoiceReqdinMultplCurrency}
									</item>
								</grammar>
								<action varName="IsInvoiceReqdinMultplCurrency" operator="SET_TO">
									{IsInvoiceReqdinMultplCurrency.value:main}
								</action>
								<goto ref="output_2443148" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsDataPrivacyConfirmed -->
					<output id="output_2443148">
						<prompt>
							<item>Thanks , Can you Confirm if Data Privacy and Outsorting applies to your
								invoices. (it is a common requirement for Germany?)
							</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsDataPrivacyConfirmed)={IsDataPrivacyConfirmed}
									</item>
								</grammar>
								<action varName="IsDataPrivacyConfirmed" operator="SET_TO">{IsDataPrivacyConfirmed.value:main}
								</action>
								<goto ref="output_2443149" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsCrossBrdrInvoiceReqd -->
					<output id="output_2443149">
						<prompt>
							<item>Thanks, Is Cross border invoicing in Scope for your Client?</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsCrossBrdrInvoiceReqd)={IsCrossBrdrInvoiceReqd}
									</item>
								</grammar>
								<action varName="IsCrossBrdrInvoiceReqd" operator="SET_TO">{IsCrossBrdrInvoiceReqd.value:main}
								</action>
								<goto ref="output_2443150" />
							</input>
						</getUserInput>
					</output>

					<!-- For IsInvoiceRangeReqd -->
					<output id="output_2443150">
						<prompt>
							<item>Thanks, Can you confirm if invoice range is a requirement for your project ?
							</item>
						</prompt>

						<getUserInput>
							<input>
								<grammar>
									<item>$(Entity_IsInvoiceRangeReqd)={IsInvoiceRangeReqd}
									</item>
								</grammar>
								<action varName="IsInvoiceRangeReqd" operator="SET_TO">{IsInvoiceRangeReqd.value:main}
								</action>
								<goto ref="output_2443151" />
							</input>
						</getUserInput>
					</output>

					<output id="output_2443151">
						<if>
							<cond varName="IsInvoiceReqd" operator="EQUALS">YES</cond>
							<prompt>
								<item>Thank you for your valuable time and information,Your TPA assessment is completed
								, We will analyze and share you the next step by email</item>
							</prompt>
						</if>
						<prompt>
							<item>Sorry, TPA can not be completed. Please try again </item>
						</prompt>
					</output>

				</input>
			</folder>
		</folder>
		<folder label="Storage" id="Storage"></folder>


		<folder label="Concepts" id="Concepts"></folder>

	</flow>

	<variables>
		<var_folder name="Home">
			<var name="ClientName" type="TEXT" description="The user's Name"
				initValue="friend" />
			<var name="CountryScope" type="TEXT" description="The Country's Scope" />
			<var name="Contact" type="NUMBER" description="The Country's Scope" />
			<var name="Brand" type="TEXT" description="The Country's Scope" />
			<var name="Message" type="TEXT" description="The Country's Scope" />
			<var name="IsPaperSampleAvailable" type="TEXT" description="The Country's Scope" />
			<var name="CustomerNum" type="NUMBER" description="The Country's Scope" />
			<var name="HVECScope" type="TEXT" description="The Country's Scope" />
			<var name="IsRevenueReportThere" type="TEXT" description="The Country's Scope" />
			<var name="SiteID" type="TEXT" description="The Country's Scope" />
			<var name="IBMEntity" type="TEXT" description="The Country's Scope" />
			<var name="EPV" type="TEXT" description="The Country's Scope" />
			<var name="IsMsgImplGuideMandatory" type="TEXT" description="The Country's Scope" />
			<var name="EnvoicingSolution" type="TEXT" description="The Country's Scope" />
			<var name="IsTechSampleMandatory" type="TEXT" description="The Country's Scope" />
			<var name="IsInvoiceReqd" type="TEXT" description="The Country's Scope" />
			<var name="IsInvoiceReqdinMultplCurrency" type="TEXT"
				description="The Country's Scope" />
			<var name="IsDataPrivacyConfirmed" type="TEXT" description="The Country's Scope" />
			<var name="IsCrossBrdrInvoiceReqd" type="TEXT" description="The Country's Scope" />
			<var name="IsInvoiceRangeReqd" type="TEXT" description="The Country's Scope" />
		</var_folder>
	</variables>
	<entities>
		<entity name="Entity_ClientName">
			<value name="Name" value="Name">
				<grammar>
					<item>*</item>
				</grammar>
			</value>
		</entity>
		<entity name="Entity_CountryScope">
			<value name="USA" value="USA">
				<grammar>
					<item>US</item>
					<item>USA</item>
					<item>us</item>
					<item>usa</item>
				</grammar>
			</value>
			<value name="France" value="France">
				<grammar>
					<item>fr</item>
					<item>Fr</item>
					<item>Fran</item>
					<item>fran</item>
				</grammar>
			</value>
			<value name="Germany" value="Germany">
				<grammar>
					<item>gr</item>
					<item>Gr</item>
					<item>Ge</item>
					<item>ge</item>
				</grammar>
			</value>
			<value name="Netherlands" value="Netherlands">
				<grammar>
					<item>NL</item>
					<item>Nether</item>
					<item>nl</item>
					<item>nether</item>
				</grammar>
			</value>
			<value name="Belgium" value="Belgium">
				<grammar>
					<item>be</item>
					<item>Be</item>
					<item>Belgium</item>
					<item>BE</item>
				</grammar>
			</value>
			<value name="UK" value="UK">
				<grammar>
					<item>uk</item>
					<item>UK</item>
					<item>Uk</item>
					<item>united</item>
				</grammar>
			</value>
		</entity>
		<entity name="Entity_Contact">
			<value name="Name" value="Name">
				<grammar>
					<item>*</item>
				</grammar>
			</value>
		</entity>
		<entity name="Entity_Brand">
			<value name="Configured Hardware (System z/i/p and/or Storage)"
				value="Configured Hardware (System z/i/p and/or Storage)">
				<grammar>
					<item>Configured_Hardware_(System z/i/p and/or Storage)</item>
				</grammar>
			</value>
			<value name="System x Hardware" value="System x Hardware">
				<grammar>
					<item>System_x_Hardware</item>
				</grammar>
			</value>
			<value name="Distributed Software" value="Distributed Software">
				<grammar>
					<item>Distributed_Software</item>
				</grammar>
			</value>
			<value name="Entitled Software" value="Entitled Software">
				<grammar>
					<item>Entitle_Software</item>
				</grammar>
			</value>
			<value name="Services" value="Services">
				<grammar>
					<item>Services</item>
				</grammar>
			</value>
		</entity>
		<entity name="Entity_IsPaperSampleAvailable">
			<value name="Yes" value="Yes">
				<grammar>
					<item>Yes</item>

				</grammar>
			</value>
			<value name="No" value="No">
				<grammar>
					<item>No</item>

				</grammar>
			</value>
		</entity>
		<entity name="Entity_Message">
			<value name="Order Confirmation" value="Order_Confirmation">
				<grammar>
					<item>Order_Confirmation</item>

				</grammar>
			</value>
			<value name="Order Response" value="Order_Response">
				<grammar>
					<item>Order_Response</item>

				</grammar>
			</value>
			<value name="Advanced Shipment Notice" value="Advanced Shipment Notice">
				<grammar>
					<item>Advanced Shipment Notice</item>

				</grammar>
			</value>
			<value name="Orders and Change Orders" value="Orders and Change Orders">
				<grammar>
					<item>Orders and Change Orders</item>

				</grammar>
			</value>
			<value name="Invoice and Credit" value="Invoice and Credit">
				<grammar>
					<item>Invoice and Credit</item>

				</grammar>
			</value>
		</entity>
		<entity name="Entity_CustomerNum">
			<value name="Name" value="Name">
				<grammar>
					<item>*</item>
				</grammar>
			</value>
		</entity>
		<entity name="Entity_HVECScope">
			<value name="Yes" value="Yes">
				<grammar>
					<item>Yes</item>

				</grammar>
			</value>
			<value name="No" value="No">
				<grammar>
					<item>No</item>

				</grammar>
			</value>
		</entity>
		<entity name="Entity_IsRevenueReportThere">
			<value name="Yes" value="Yes">
				<grammar>
					<item>Yes</item>

				</grammar>
			</value>
			<value name="No" value="No">
				<grammar>
					<item>No</item>

				</grammar>
			</value>
		</entity>
		<entity name="Entity_SiteID">
			<value name="Name" value="Name">
				<grammar>
					<item>*</item>
				</grammar>
			</value>
		</entity>
		<entity name="Entity_IBMEntity">
			<value name="Name" value="Name">
				<grammar>
					<item>*</item>
				</grammar>
			</value>
